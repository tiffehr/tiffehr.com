{
  "name": "tiffehr.github.io",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "robots": "node scripts/robots.ts",
    "prepare": "svelte-kit sync || echo ''",
    "build": "vite build && cp CNAME build/",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t",
    "clean": "rm -rf build",
    "clean:all": "rm -rf build node_modules",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "eslint . --fix",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fontsource/grand-hotel": "^5.2.8",
    "@fontsource/merriweather": "^5.2.11"
  },
  "devDependencies": {
    "@darkvisitors/sdk": "^1.5.0",
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^4.0.5",
    "@types/eslint": "^8.56.0",
    "@types/node": "^20",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass-embedded": "^1.95.1",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "tslib": "^2.4.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.16"
  }
}
